    <joint name="upperlegM3_lowerlegM3_joint" type="revolute">
        <parent link="upperleg_M3_link"/>
        <child link="lowerleg_M3_link"/>
        <origin xyz="0 0.0095 0.06" rpy="0 0 3.14159"/>
